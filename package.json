{
  "name": "TODO",
  "description": "TODO",
  "version": "0.0.1",
  "scripts": {
    "dev": "dotenv -e env/development.env -- sapper dev",
    "build": "dotenv -e env/production.env -- sapper build",
    "export": "dotenv -e env/production.env -- sapper export",
    "start": "node __sapper__/build",
    "-- TEST --": "-------------------------------------------------------------------------------------------------------",
    "test": "jest -i --verbose --colors",
    "test:fakeci": "dotenv -e test.env -- yarn test",
    "test:coverage": "yarn test --coverage",
    "test:send-coverages": "yarn coverage:codecovs && yarn coverage:coveralls && yarn coverage:codacy",
    "coverage:codecovs": "curl -s https://codecov.io/bash | bash",
    "coverage:coveralls": "cat ./coverage/lcov.info | coveralls",
    "coverage:codacy": "curl -fL https://coverage.codacy.com/get.sh -o get.sh && bash get.sh report -l typescript -r coverage/lcov.info",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "run-p --race dev cy:run",
    "-- MOCK --": "-------------------------------------------------------------------------------------------------------",
    "start:mock": "nodemon --watch 'mock/**/*.ts' --exec 'ts-node' mock/server.ts",
    "-- HELPERS --": "-------------------------------------------------------------------------------------------------------",
    "ci": "yarn clean && yarn install",
    "clean": "yarn clean:packages",
    "clean:packages": "rm -fr yarn.lock package-lock.json dist node_modules",
    "eslint": "eslint -c .eslintrc.js --ext .ts --ext .js src",
    "eslint:fix": "yarn eslint --fix",
    "format": "pretty-quick \"src/**/*.{ts,svelte}\" --write --staged",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "polka": "next",
    "sirv": "^0.4.2",
    "svelte-material-ui": "1.0.0-beta.21"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
